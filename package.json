  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "turbo": "latest"
  },
  "packageManager": "pnpm@9.1.2",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
