      "sharp@<0.32.6": ">=0.32.6",
      "nodemailer@<=6.9.8": ">=6.9.9",
      "axios@>=0.8.1 <0.28.0": ">=0.28.0",
      "follow-redirects@<=1.15.5": ">=1.15.6",
      "tar@<6.2.1": ">=6.2.1",
      "undici@<5.28.4": ">=5.28.4",
