{
  "name": "yakoo-strapi-local-starter",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"apps/web/**/*.{ts,tsx,md}\""
