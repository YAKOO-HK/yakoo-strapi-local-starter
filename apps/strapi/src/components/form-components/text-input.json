      "default": "text"
    },
    "label": {
      "type": "string"
    },
    "placeholder": {
      "type": "string"
    },
    "required": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "regExp": {
      "type": "string"
    }
  }
}
